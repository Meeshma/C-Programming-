#include <stdio.h>

int main() {
    // Declare variables of different data types
    int integer;
    float floating;
    double doublePrecision;
    char character;

    // Print the memory size of each data type
    printf("Size of int: %zu bytes\n", sizeof(integer));
    printf("Size of float: %zu bytes\n", sizeof(floating));
    printf("Size of double: %zu bytes\n", sizeof(doublePrecision));
    printf("Size of char: %zu bytes\n", sizeof(character));

    return 0;
}
