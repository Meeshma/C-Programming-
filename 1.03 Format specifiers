#include <stdio.h>

int main() {
    int integer = 42;
    float floating = 3.14159;
    double doublePrecision = 2.718281828459;
    char character = 'A';
    char string[] = "Hello, World!";

    // Print variables using format specifiers
    printf("Integer: %d\n", integer);
    printf("Floating point: %.2f\n", floating);
    printf("Double precision: %.10lf\n", doublePrecision);
    printf("Character: %c\n", character);
    printf("String: %s\n", string);

    return 0;
}
