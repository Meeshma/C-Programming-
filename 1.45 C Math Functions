#include <stdio.h>
#include <math.h>

int main() {
    // Declare variables
    double num1 = 9.0, num2 = 2.0;

    // Square root
    printf("Square root of %.2f is %.2f\n", num1, sqrt(num1));

    // Power
    printf("%.2f raised to the power of %.2f is %.2f\n", num1, num2, pow(num1, num2));

    // Exponential
    printf("Exponential of %.2f is %.2f\n", num2, exp(num2));

    // Natural logarithm
    printf("Natural logarithm of %.2f is %.2f\n", num1, log(num1));

    // Base-10 logarithm
    printf("Base-10 logarithm of %.2f is %.2f\n", num1, log10(num1));

    // Absolute value
    printf("Absolute value of %.2f is %.2f\n", -num1, fabs(-num1));

    // Ceiling
    printf("Ceiling of %.2f is %.2f\n", num1, ceil(num1));

    // Floor
    printf("Floor of %.2f is %.2f\n", num1, floor(num1));

    // Sine
    printf("Sine of %.2f radians is %.2f\n", num2, sin(num2));

    // Cosine
    printf("Cosine of %.2f radians is %.2f\n", num2, cos(num2));

    // Tangent
    printf("Tangent of %.2f radians is %.2f\n", num2, tan(num2));

    return 0;
}
