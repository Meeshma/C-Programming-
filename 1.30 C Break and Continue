#include <stdio.h>

int main() {
    // Break statement example
    printf("Break statement example:\n");
    for (int i = 1; i <= 10; i++) {
        if (i == 5) {
            break; // Exit the loop when i is 5
        }
        printf("%d\n", i);
    }

    // Continue statement example
    printf("\nContinue statement example:\n");
    for (int i = 1; i <= 10; i++) {
        if (i == 5) {
            continue; // Skip the rest of the loop when i is 5
        }
        printf("%d\n", i);
    }

    return 0;
}
